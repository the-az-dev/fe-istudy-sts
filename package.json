{
  "name": "fe-istudy-sts",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:bun": "bunx --bun vite --port 8080 -- --host",
    "dev:node": "npx vite --port 8080 -- --host",
    "build": "vue-tsc -b && npx vite build",
    "build:bun": "vue-tsc -b && bunx --bun vite build",
    "build:gh:bun": "bun run build:bun --github && cp dist/index.html dist/404.html && git subtree push --prefix dist origin gh-pages && gh-pages -d dist",
    "build:gh:node": "bun run build --gihub && cp dist/index.html dist/404.html && git subtree push --prefix dist origin gh-pages && gh-pages -d dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.12",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vue-tsc": "^2.2.12"
  }
}
